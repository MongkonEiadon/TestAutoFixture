<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net472</TargetFrameworks>
    <TreatWarningsAsErrors>True</TreatWarningsAsErrors>
    <GenerateAssemblyInfo>True</GenerateAssemblyInfo>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>TestAutoFixture</Title>
    <Authors>Mongkon Eiadon</Authors>
    <NeutralLanguage>en-US</NeutralLanguage>
    <PackageReleaseNotes>Using Class under Test (CUT)</PackageReleaseNotes>
    <NoWarn>1701;1702;1705;1591</NoWarn>
    <Version>1.1.0</Version>
    <PackageProjectUrl>https://github.com/MongkonEiadon/TestAutoFixture</PackageProjectUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AutoFixture.AutoMoq" Version="4.8.0" />
    <PackageReference Include="Moq" Version="4.10.1" />
    <PackageReference Include="NUnit" Version="3.11.0" />
  </ItemGroup>

</Project>
